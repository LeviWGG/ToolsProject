<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="app.mian.wangliwei.toolsproject.view.SettingActivity">

    <Button
        android:id="@+id/btn_clear_memory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Clear"/>

    <Button
        android:id="@+id/btn_get_foodnum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/btn_clear_memory"
        android:text="GetFoodNum"/>

    <Button
        android:id="@+id/btn_add_foodnum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/btn_get_foodnum"
        android:text="AddFoodNum"/>

</RelativeLayout>
